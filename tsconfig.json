{
    "include": [
      "***/**/*.ts"
    ],
    "exclude": [],
    "compilerOptions": {
      "target": "ESNext",
      "lib": [
        "ESNext",
        "DOM"
      ],
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "noImplicitAny": false,
      "types": [
        "node",
        "cypress",
        "cypress-xpath"
      ]
    }
  }